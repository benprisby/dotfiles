eval "$(/opt/homebrew/bin/brew shellenv)"

export PATH="${HOME}/Qt/5.15.2/clang_64/bin:${PATH}"

alias ax="arch -x86_64"
alias bu="brew update --verbose && brew upgrade --verbose && brew upgrade --verbose --cask --greedy && brew cleanup"
alias ll="ls -lahG"
alias uuid="uuidgen | tr '[:upper:]' '[:lower:]'"
